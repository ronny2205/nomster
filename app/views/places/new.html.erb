<div class="row">
 <div class="booyah-box">
	<h1>Tell us about the restaurant</h1>
	<br/>
	<%= simple_form_for @place do |f| %>
    	<%= f.input :name %>
    	<br/>
    	<%= f.input :address %>
    	<br/>
    	<%= f.input :description %>
    	<br/>
    	<%= f.submit 'Create', :class => 'btn btn-success' %>
  	<% end %>
  </div>  
</div>	